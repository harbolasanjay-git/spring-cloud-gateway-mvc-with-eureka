spring:
  application:
    name: api-gateway
  cloud:
    gateway:
      mvc:
        routes:
        - id: order-service
          uri: order-service
          predicates: 
            - Path=/orders/**
        - id: user-service
          predicates:
            - Path=/users/**
server:
  ssl:
    enabled: false
order-service:
  base-url: http://localhost:8081